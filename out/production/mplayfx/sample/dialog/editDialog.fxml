<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane fx:id="dialogPane" xmlns:fx = "http://javafx.com/fxml/1" fx:controller="sample.dialog.DialogController"
prefWidth="800" prefHeight="400">
    <headerText>Edit A Track</headerText>
    <content>
        <GridPane>
            <Label text="Album Artist: " GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <TextField fx:id="artist" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
            <Label text="Album Name: " GridPane.rowIndex="4" GridPane.columnIndex="1"/>
            <TextField fx:id="albumFieldName" GridPane.rowIndex="4" GridPane.columnIndex="2"/>
            <!--<TextField fx:id = "trackTitle" GridPane.rowIndex="3" GridPane.columnIndex="2"/>-->
        </GridPane>
    </content>
</DialogPane>
